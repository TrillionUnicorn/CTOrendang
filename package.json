{
	"name": "ctorendang",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"description": "AI + Human CTO Platform - Democratizing access to world-class technical leadership",
	"author": "Hunter Ho <42632102+HunterHo07@users.noreply.github.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/TrillionUnicorn/CTOrendang.git"
	},
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "playwright test",
		"test:ui": "playwright test --ui",
		"test:headed": "playwright test --headed",
		"test:debug": "playwright test --debug",
		"docker:build": "docker build -t ctorendang:latest .",
		"docker:run": "docker run -p 3000:3000 ctorendang:latest",
		"docker:compose": "docker-compose up -d",
		"docker:down": "docker-compose down"
	},
	"devDependencies": {
		"@playwright/test": "^1.56.0",
		"@sveltejs/adapter-auto": "^5.0.0",
		"@sveltejs/adapter-node": "^5.3.3",
		"@sveltejs/kit": "^2.46.5",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@types/three": "^0.180.0",
		"autoprefixer": "^10.4.21",
		"gsap": "^3.13.0",
		"lottie-web": "^5.13.0",
		"playwright": "^1.56.0",
		"postcss": "^8.5.6",
		"svelte": "^5.39.12",
		"svelte-check": "^4.0.0",
		"svelte-motion": "^0.12.2",
		"tailwindcss": "^3.4.18",
		"three": "^0.180.0",
		"typescript": "^5.0.0",
		"vite": "^6.3.6"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"tailwind-merge": "^3.3.1"
	}
}
