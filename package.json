{
	"name": "ctorendang",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"description": "AI + Human CTO Platform - Democratizing access to world-class technical leadership",
	"author": "Hunter Ho <42632102+HunterHo07@users.noreply.github.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/TrillionUnicorn/CTOrendang.git"
	},
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "playwright test",
		"test:ui": "playwright test --ui",
		"test:headed": "playwright test --headed",
		"test:debug": "playwright test --debug",
		"test:integration": "playwright test tests/integration",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"db:seed": "bun run prisma/seed.ts",
		"db:backup": "bun run scripts/backup-database.ts",
		"docker:build": "docker build -t ctorendang:latest .",
		"docker:run": "docker run -p 3000:3000 ctorendang:latest",
		"docker:compose": "docker-compose up -d",
		"docker:down": "docker-compose down",
		"deploy": "vercel --prod",
		"deploy:staging": "vercel"
	},
	"devDependencies": {
		"@playwright/test": "^1.56.0",
